{
  "name": "node_streams",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "test": "pnpm typeCheck && pnpm lint && pnpm jest",
    "start": "",
    "jest": "jest",
    "typeCheck": "tsc --noEmit",
    "test:watch": "jest --watch --coverage=false",
    "build": "NODE_ENV=production pnpm build:clean && tsc -p tsconfig.build.json && tsc-alias",
    "build:clean": "rm -rf dist",
    "clean": "rm -rf node_modules dist coverage",
    "lint": "eslint {src,tests} --ext .js,.jsx,.ts,.tsx",
    "lintStaged": "lint-staged --config .lint-staged.config",
    "prepublishOnly": "pnpm lint && pnpm test && pnpm build",
    "start:dev": "nodemon",
    "start:prod": "NODE_ENV=production node --enable-source-maps dist/index.js",
    "start:debug": "nodemon --inspect-brk"
  },
  "keywords": [],
  "author": "Lei Zhao <leo.zhao.real@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.2",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.6",
    "typescript": "^5.0.4",
    "typescript-plugin-css-modules": "^5.0.1",
    "webpack-merge": "^5.9.0"
  },
  "dependencies": {
    "dayjs": "^1.11.7",
    "lodash-es": "^4.17.21",
    "log4js": "^6.9.1"
  },
  "files": [
    "dist/**/*"
  ],
  "engines": {
    "node": ">=v16.17.1"
  }
}