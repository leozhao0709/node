    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "acorn": "^8.11.3",
    "browserslist": "^4.22.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
