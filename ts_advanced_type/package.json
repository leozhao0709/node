    "lintStaged": "lint-staged --config .lint-staged.config.js",
    "prepublishOnly": "pnpm lint && pnpm test && pnpm build",
    "start:dev": "nodemon",
    "start:prod": "NODE_ENV=production node --enable-source-maps dist/index.js",
    "start:debug": "nodemon --inspect-brk"
