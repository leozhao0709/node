{
  "name": "mycv_nest",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "test": "pnpm typeCheck && pnpm lint && pnpm jest && pnpm jest:e2e",
    "start": "",
    "jest": "jest",
    "jest:e2e": "jest --config ./jest.e2e.config.cjs",
    "typeCheck": "tsc --noEmit",
    "test:watch": "jest --watch --coverage=false",
    "build": "NODE_ENV=production pnpm build:clean && tsc -p tsconfig.build.json && tsc-alias",
    "build:clean": "rm -rf dist",
    "clean": "rm -rf node_modules dist coverage",
    "lint": "eslint {src,tests} --ext .js,.jsx,.ts,.tsx",
    "lintStaged": "lint-staged --config .lint-staged.config.cjs",
    "prepublishOnly": "pnpm lint && pnpm test && pnpm build",
    "start:dev": "nodemon",
    "start:prod": "NODE_ENV=production node --enable-source-maps dist/index.js",
    "start:debug": "nodemon --inspect-brk",
    "console": "node dist/console/console.js",
    "db:migrate:init": "mikro-orm-esm migration:create --initial  --name",
    "db:migrate:create": "mikro-orm-esm migration:create --name",
    "db:migrate:up": "mikro-orm-esm migration:up",
    "db:migrate:down": "mikro-orm-esm migration:up",
    "db:seeder:create": "mikro-orm-esm seeder:create",
    "db:seeder:run": "mikro-orm-esm seeder:run",
    "db:seeder:fresh": "mikro-orm-esm migration:fresh --seed"
  },
  "keywords": [],
  "author": "Lei Zhao <leo.zhao.real@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@nestjs/cli": "^9.5.0",
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/testing": "^9.4.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^20.2.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.2",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.6",
    "ttypescript": "^1.5.15",
    "typescript": "^5.0.4",
    "typescript-plugin-css-modules": "^5.0.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@mikro-orm/cli": "^5.7.9",
    "@mikro-orm/core": "^5.7.9",
    "@mikro-orm/migrations": "^5.7.9",
    "@mikro-orm/mysql": "^5.7.9",
    "@mikro-orm/nestjs": "^5.1.8",
    "@mikro-orm/seeder": "^5.7.9",
    "@nestjs/common": "^9.4.1",
    "@nestjs/config": "^2.3.2",
    "@nestjs/core": "^9.4.1",
    "@nestjs/mapped-types": "^1.2.2",
    "@nestjs/platform-express": "^9.4.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "lodash-es": "^4.17.21",
    "log4js": "^6.9.1",
    "mysql2": "^3.3.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "files": [
    "dist/**/*"
  ],
  "engines": {
    "node": ">=v16.17.1"
  },
  "type": "module",
  "pnpm": {
    "overrides": {
      "@types/superagent": "4.1.14"
    }
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/db/mikro-orm.config.ts",
      "./dist/db/mikro-orm.config.js"
    ]
  }
}